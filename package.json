{
  "name": "vesper-subgraph",
  "version": "1.0.0",
  "private": true,
  "description": "Vesper Subgraph.",
  "main": "index.js",
  "scripts": {
    "build": "graph build",
    "create": "graph create bloqpriv/vesper-subgraph --node https://api.thegraph.com/deploy/",
    "create-local": "graph create bloqpriv/vesper-subgraph --node http://127.0.0.1:8020",
    "codegen": "graph codegen",
    "dev": "npm run bootstrap && npm run create-local && npm run deploy-local",
    "deploy": "graph deploy bloqpriv/vesper-subgraph --ipfs https://api.thegraph.com/ipfs/ --node https://api.thegraph.com/deploy/",
    "deploy-local": "graph deploy bloqpriv/vesper-subgraph --ipfs http://localhost:5001 --node http://127.0.0.1:8020 -l 0.0.1",
    "format:fix": "prettier --write",
    "bootstrap": "node subgraph-generator.js",
    "prepare": "husky install"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bloqpriv/vesper-subgraph.git"
  },
  "keywords": [
    "Vesper",
    "TheGraph"
  ],
  "author": {
    "name": "Gonzalo D'Elia",
    "email": "gonzalo.n.delia@gmail.com"
  },
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/bloqpriv/vesper-subgraph/issues"
  },
  "homepage": "https://github.com/bloqpriv/vesper-subgraph#readme",
  "devDependencies": {
    "@graphprotocol/graph-cli": "^0.21.1",
    "@graphprotocol/graph-ts": "^0.20.0",
    "fs-extra": "^10.0.0",
    "husky": "^7.0.1",
    "lint-staged": "^11.1.2",
    "p-tap": "^3.1.0",
    "prettier": "^2.3.2"
  },
  "dependencies": {
    "vesper-metadata": "2.16.0"
  },
  "prettier": {
    "singleQuote": true
  },
  "lint-staged": {
    "*.{js,ts,md,json}": "npm run format:fix"
  }
}
